<h1>Boards#show</h1>
<p>Find me in app/views/boards/show.html.erb</p>

<%= link_to("Create Post", new_board_post_path(@board)) %>
</br>

<% @posts.each do | post | %>
	<tr>
		<td> <%= post.type_name %> </td>
		<td> <%= link_to(post.title, board_post_path(@board, post)) %> </td>
		<td> <%= post.content %> </td>
		</br>
	</tr>
<% end %>